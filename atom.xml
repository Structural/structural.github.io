<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <id>tag:structur.al,2013-08-06:00000</id>
  <title>Structural</title>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <link rel="self" type="application/atom+xml" href="http://structur.al/atom.xml" />
  <link rel="alternate" type="text/html" href="http://structur.al" />

  {% for post in site.posts limit:10 %}
    <entry>
      <title>{{ post.title }}</title>
      <author>
        <name>{{ post.author }}</name>
      </author>
      <content type="text/html" src="http://structur.al{{ post.url }}">
      </content>
      <summary>{{ post.excerpt | strip_html }}</summary>
      <id>tag:structur.al,{{ post.date | date: "%Y-%m-%d" }}:{{ post.url }}</id>
      <link rel="alternate" type="text/html" href="http://structur.al{{ post.url }}" />
      <published>{{ post.date | date_to_xmlschema }}</published>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
    </entry>
  {% endfor %}
</feed>
